        <bpmn:flowNodeRef>Task_04</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_05</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Modellqualitaet</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_10</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Kollisionen</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_12</bpmn:flowNodeRef>
      </bpmn:lane>
